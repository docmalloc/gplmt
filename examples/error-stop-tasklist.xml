<?xml version="1.0" encoding="utf-8"?>
<experiment>
  
  <description>Hello World in GPLMT</description>

  <targets>
    <target name="local" type="local" />
  </targets>

  <tasklists>
    <tasklist name="tl1" on-error="stop-tasklist">
	<seq>
		<fail status="failed"/>
      		<run>echo "This command should not run (after fail)"</run>
	</seq>
    </tasklist>
    <tasklist name="tl2">
      <seq><run>echo "This command should run"</run></seq>
    </tasklist>
  </tasklists>

  <steps>
    <step tasklist="tl1" targets="local" />
    <synchronize />
    <step tasklist="tl2" targets="local" />
  </steps>
</experiment>
